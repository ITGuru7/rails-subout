= simple_form_for @new_supplier do |f|
  = f.input :created_from_invitation_id, :as => :hidden
  = f.input :name, label: "Company name"
  = f.input :email, label: "Company email"
  = f.input :password
  = f.input :password_confirmation
  = f.input :street_address
  = f.input :zip_code
  = f.input :city
  = f.input :state

  = f.button :submit
